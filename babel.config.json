{
  "presets": [
   
  ],
  "comments": false,
  "overrides": [
    {
      "test": [ "./src/server/pages/**" ],
      "presets": [ "preact" ]
    },
    {
      "test": [ "./src/**" ],
      "exclude": [ "./src/client/**" ],
